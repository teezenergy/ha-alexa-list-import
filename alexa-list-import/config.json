{
  "name": "Alexa List Import",
  "version": "2.0.22",
  "slug": "alexa_list_import",
  "description": "Imports the Alexa shopping list into Home Assistant",
  "url": "https://github.com/teezenergy/ha-alexa-list-import",
  "arch": [
    "amd64",
    "aarch64",
    "armv7",
    "armhf",
    "i386"
  ],
  "startup": "services",
  "boot": "auto",
  "init": false,

  "options": {
    "amazon_email": "",
    "amazon_password": "",
